{
	"env": {
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": "./node_modules/standard/eslintrc.json",
	"rules": {
		"semi": [2, "always"],
		"indent": [2, "tab"],
		"react/jsx-indent": [2, "tab"],
		"no-tabs": 0,
		"curly": [2, "multi"],
		"react/jsx-indent-props": [2, "tab"],
		"no-unused-vars": "warn"
	}
}